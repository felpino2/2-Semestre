#Recebe numero de alunos Dar nome aos aluos, suas notas
Na1 = int
A1 = []
A1appendor = str
Notas = []
Notasappendor = int

# Dar Valor as notas

Na1=int(input("Quantos alunos? "))

for i in range(Na1):
    A1appendor = str(input("Digite o nome do aluno "))
    A1.append(A1appendor)
    Notasappendor = int(input("Qual sua respectiva nota? "))
    Notas.append(Notasappendor)


for i in range(len(Notas)):
    if Notas[i] >= 9:
        Notas[i] = 'A'
    elif Notas[i] >= 8:
        Notas[i] >= 'B'
    elif Notas[i] >= 6:
        Notas[i] = 'C'
    elif Notas[i] >= 4:
        Notas[i] = 'D'
    elif Notas[i] >= 2:
        Notas[i] = 'E'
    else:
        Notas[i] = 'F'

for i in range(len(Notas)):
    print("Aluno: " , A1[i],"Nota", Notas[i])
