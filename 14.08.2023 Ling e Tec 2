Notas = []
nome = []

def notanome():
    Na1 = int(input("Quantos alunos? "))
    for i in range(Na1):
        A1appendor = str(input("Digite o nome do aluno "))
        nome.append(A1appendor)
        Notasappendor = int(input("Qual sua respectiva nota? "))
        Notas.append(Notasappendor)

def avalianotas():
    for i in range(len(Notas)):
        if Notas[i] >= 9:
            Notas[i] = 'A'
        elif Notas[i] >= 8:
            Notas[i] >= 'B'
        elif Notas[i] >= 6:
            Notas[i] = 'C'
        elif Notas[i] >= 4:
            Notas[i] = 'D'
        elif Notas[i] >= 2:
            Notas[i] = 'E'
        else:
            Notas[i] = 'F'

notanome()
avalianotas()

for i in range(len(Notas)):
    print("Aluno: " , nome[i],"Nota", Notas[i])
    print("Aluno: " , nome[i],"Nota", Notas[i])
