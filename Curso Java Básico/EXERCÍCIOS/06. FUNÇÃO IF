package Main;
import java.util.Scanner;

public class MAIN {
	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);
		System.out.println("Escreva o número:");
		int x = input.nextInt();
		mult(x);
		
	}

	public static void mult(int x) {

		if((x%2)==0) {
			System.out.println("É Mltiplo de 2.");
		}else {
			System.out.println("Não é multiplo de 2.");
		}

	}

}
