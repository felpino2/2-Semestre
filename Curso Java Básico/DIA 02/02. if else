package main;
import java.util.Scanner;
public class exemplo {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("X: ");
		int x = sc.nextInt();
		if (x > 2) {
			System.out.println("X é maior que dois.");
		} else if (x < 2) {
			System.out.println("X é menor que dois.");
		} else {
			System.out.println("X é dois.");
		}
		sc.close();
	}
}
