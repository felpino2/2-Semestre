#include <stdio.h>

int main(){

int matriz1[3][2];
int matriz2[3][2];
int matriz3[3][2];

for (int i = 0; i < 3; i++){
    for (int j = 0; j < 2; j++){
        printf("Qual o termo [%d][%d] da 1\n", i, j);
        scanf("%d", &matriz1[i][j]);
    }
}
for (int i = 0; i < 3; i++){
    for (int j = 0; j < 2; j++){
        printf("Qual o termo [%d][%d] da 2\n", i, j);
        scanf("%d", &matriz2[i][j]);
        matriz3[i][j] = matriz1[i][j] + matriz2[i][j];
    }
}
for (int i = 0; i < 3; i++){
    for (int j = 0; j < 2; j++){
        printf("[%d] ", matriz3[i][j]);
    }
    printf("\n");
}


}
