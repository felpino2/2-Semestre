#include <stdio.h>

int main(){

int vetor1[5];
int vetor2[5];
int soma = 0;

for (int i = 0; i<5; i++){
    printf("Qual o termo %d do vetor 1?\n", i+1);
    scanf("%d", &vetor1[i]);
}

for (int i = 0; i<5; i++){
    printf("Qual o termo %d do vetor 2?\n", i+1);
    scanf("%d", &vetor2[i]);
}

for (int i = 0; i<5; i++){
    soma += (vetor1[i]*vetor2[i]);
}

printf("O produto escalar Ã© %d", soma);

}
