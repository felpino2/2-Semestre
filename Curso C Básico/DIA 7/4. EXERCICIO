#include <stdio.h>
#include <locale.h>

void main() {
setlocale(LC_ALL, "Portuguese");

int i, j;
int matriz[3][3];

for (i =0; i < 3; i++){
    for (j=0; j < 3; j++){
        printf("Digite o elemento da linha %d e coluna %d \n", i, j);
        scanf("%d", &matriz[i][j]);
    }
}
for (i =0; i < 3; i++){
    for (j=0; j < 3; j++){
        printf("[%d] ", matriz[i][j]);
    }
    printf("\n");
}


}
