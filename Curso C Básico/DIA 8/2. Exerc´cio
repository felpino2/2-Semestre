#include <stdio.h>

void main(){

int n, vetor[n], N;
int *p;

printf("Qual tamanho do vetor?\n");
scanf("%d", &n);

for (int i = 0; i < n; i++){
    printf("Insira o termo\n");
    scanf("%d", &vetor[i]);
    }


for (int i = 0; i < n; i++){
    printf("%d\n", vetor[i]);
    }


p = &vetor;
printf("%p\n", *p);


}
