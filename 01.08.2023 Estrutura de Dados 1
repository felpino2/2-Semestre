	// Programa de Ordenação Direta

#import <stdio.h>
#import <stdlib.h>
#import <time.h>

	// Decl. de Variáveis
	int vetor[10000];
	int i = 0;
	int j = 0;
	time_t t;
	int temp;


// Função Principal de Execução de Programa

int main () {
	//Inicialização de aleatoriedade
	srand((unsigned) time(&t));
	
	// Inicialização do Vetor
	for (i = 0; i < 10000 ; i++){
	vetor[i] = (rand() % 10000);
}
	// Impressão do Vetor
	for (i = 0; i < 10000 ; i++){
		printf("%d\n", vetor[i]);
	}

	// Ordenação Propriamente Dita
	for (i = 0; i < 9999 ; i++) // Compara primeiro ao penúltimo
		for (j = (i+1) ; j < 10000 ; j++) // Do próximo ao último {
		if (vetor[j] < vetor[i]) {
			temp = vetor[i];
			vetor[i] = vetor[j];
			vetor[j] = temp;
		}
		
	// Impressão do Vetor
	for (i = 0; i < 10000 ; i++){
		printf("%d\n", vetor[i]);
	}

	}
