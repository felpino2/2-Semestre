class Cliente():
    def __init__(self, nome, sobrenome, cpf):
        self.nome = nome
        self.sobrenome = sobrenome
        self.cpf = cpf

    def grava(c):
        f = open('cliente.txt', 'a')
        f.write(c.nome + "," + c.sobrenome + "," + c.cpf + "\n")
        f.close()

    def ler(c):
        f = open('cliente.txt', 'r')
        print('leitura de cliente')
        l = f.read()
        print(l)
