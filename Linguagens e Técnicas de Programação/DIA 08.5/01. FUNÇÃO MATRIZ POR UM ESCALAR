#função de produto da matriz M por um escalar x:

def produtoMatriz(m,x):
    for i in range(len(m)):
        for j in range(len(m[0])):
            if j == i:
                m[i][j] = m[i][j] * x
    return(m)
    
print("Produto por escalar: ",produtoMatriz(m,3))
[19:20, 16/10/2023] Gabriel Lima Ceub: def matrizQuadrada(m):
    for i in range(len(m)):
        for i in range(len(m[0])):
            resultado = m[j][i] * m[j][i]
    return resultado

matrizQuadrada(m)
