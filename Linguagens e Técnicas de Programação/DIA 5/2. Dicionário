notas = {
    'João' : 9,
    'Maria': 10,
    'José': 4
}

nome = input("Digite o nome do aluno: ")
nota = float(input("Nota dele: "))
print('Antigo dicionário: ', notas)

if notas.get(nome):
    print("Já existe o aluno ", nome)
else:
    notas[nome] = nota
print('Novo dicionário: ')
print(notas)
